<h1>Create a Survey</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@survey) do |f| %>
      <%= render 'shared/errors', object: f.object %>
      <%= f.label :title %>
      <%= f.text_field :title %>

      <%= f.label :description %>
      <%= f.text_area :description %>

      <%= f.submit "Continue", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
